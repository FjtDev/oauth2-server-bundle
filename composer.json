{
    "name": "fjtdev/oauth2-server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony OAuth2ServerBundle",
    "keywords": ["oauth", "oauth2", "security"],
    "homepage": "http://github.com/bshaffer/oauth2-server-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Shaffer",
            "email": "bshafs@gmail.com"
        },
        {
            "name": "Fjtdev",
            "email": "dev@florajet.com"
        }
    ],
    "minimum-stability":"dev",
    "require": {
        "php": ">=7.4",
        "bshaffer/oauth2-server-php": ">=1.0",
        "bshaffer/oauth2-server-httpfoundation-bridge": "^1.5",
        "sensio/framework-extra-bundle": "^6.2"
    },
    "require-dev": {
        "symfony/symfony": "5.4.x-dev",
        "doctrine/orm": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "OAuth2\\ServerBundle\\": "src/",
            "OAuth2\\ServerBundle\\Controller\\": "src/Controller"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        },
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
